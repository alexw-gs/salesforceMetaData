        Long startTS = System.currentTimeMillis();
        Long timeDiff = 0;
        do
        {
            timeDiff = System.currentTimeMillis() - startTS;
        }
        while(timeDiff <= 5*1000);  
        System.assertEquals(3, 3);
    }
    
